// COMPILER OUTPUT 69\n69\n69\n69\n69\n69\n69\n69\n
fn main() {
    println(69        ) // decimal
    println(0x45      ) // hexadecimal
    println(0o105     ) // octal
    println(0b01000101) // binary
    println(6_9        ) // decimal
    println(0x4_5      ) // hexadecimal
    println(0o1_0_5    ) // octal
    println(0b0100_0101) // binary
}